    "def fiboSer(limit):\n",
    "    count = 0\n",
    "    a,b = 0,1\n",
    "    while count<limit:\n",
    "        a,b = b,a+b\n",
    "        yield a\n",
    "        count+=1\n",
    "    "
   ]
